#------PLOT SFC Wx CHART
ingest,$$8syn\20@YYZ@MMZ@DDZ06Z.syn
ERASE
MAKE,3,34.0,125.0,5000,1920,1080,0,0,1,1

#...... PLOT RH great than 80%
ANALYZE,OVER FILL COLOR=130:255:130 EQUA=100 OVER FILL COLOR=160:255:160 GRTN=90 OVER FILL COLOR=200:255:200 GRTN=80 OVER FILL COLOR=180:250:180 GRTN=70 SADC=1:0 STOR=1 RELH Hsfc
PRODUCT,PLF,0,PLOT
STAMP
import,$$3\SFCH.png
EXPORT,$$1@YYZ@MMZ@DDZ06_SFC_RELH.gif


#........PLOT All level Wet bulb temperature (deg C) and -e, or equivalent potential temperature (deg K)
ERASE
ANALYZE,CONT CINT=2 LINE=1 DOTS COLOR=200:000:000 BULB HSFC
ANALYZE,CONT CINT=2 LINE=1 DOTS COLOR=000:000:200 THTE HSFC
PRODUCT,PLF,PLOT
STAMP
import,$$3\SFCWET.png
EXPORT,$$1@YYZ@MMZ@DDZ06_SFC_WetBulbT_ThetaE.gif

#........PLOT PRESSURE TENDENCY
ERASE
analyze,PLMN CONT CINT=4 LINE=1 DOTS COLOR=200:000:000 LSTN=0 PTEN HSFC
ANALYZE,PLMN CONT CINT=4 LINE=1 DOTS COLOR=000:000:200 GRTN=0 PTEN HSFC
